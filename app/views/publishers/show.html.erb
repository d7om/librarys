<p id="notice"><%= notice %></p>

<h1><%= @publisher.name %></h1>

<% @publisher.books.each do |book| %>
    <h3><%= link_to book.name, book%></h3>
    <h4><%= book.author.name %></h4>
<% end %>



<%= link_to 'Edit', edit_publisher_path(@publisher), class:'btn' %>
<%= link_to 'Back', publishers_path, class:'btn' %>
